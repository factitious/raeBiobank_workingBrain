issn = {14710048},
keywords = {Attention,Learning and memory,Reward,Sleep deprivation,Working memory,brain,physiology,sleep},
mendeley-tags = {brain,physiology,sleep},
month = {jul},
number = {7},
pages = {404--418},
publisher = {Nature Publishing Group},
title = {{The sleep-deprived human brain}},
volume = {18},
year = {2017}
}
@article{Kakeda2011,
abstract = {Working conditions such as shift work constitute a well-known risk factor for insomnia and excessive daytime sleepiness complaints. We compared brain gamma-aminobutyric acid (GABA), glutamic acid (Glu), glutamine (Gln), and Glx (Glu+Gln) levels in day-shift versus alternate-shift workers with proton magnetic resonance spectroscopy (1H-MRS) at 3T. The study population consisted of 32 healthy adult volunteers (16day-shift and 16 alternate-shift workers). Each subject underwent MRS conducted using a MEGA-PRESS sequence in the early morning and early evening on the same day. Spectroscopy voxels (3.0cm×3.0cm×3.0cm) were placed in the frontal lobe and parieto-occipital lobe. The GABA/Cr ratio in the frontal lobe was significantly lower for the alternate-shift group than for the day-shift group in the early evening (1.885 vs. 0.875). For the other metabolite ratios (Gln/Cr and Glx/Cr), there were no significant differences between the two groups regardless of morning or evening schedule. Our preliminary finding represents a possible alteration of GABA content in the brain related to an irregular work schedule. {\textcopyright} 2010 Elsevier Ireland Ltd.},
author = {Kakeda, Shingo and Korogi, Yukunori and Moriya, Junji and Ohnari, Norihiro and Sato, Toru and Ueno, Susumu and Yanagihara, Nobuyuki and Harada, Masafumi and Matsuda, Tsuyoshi},
doi = {10.1016/j.pscychresns.2010.10.011},
file = {:Users/munchausend/Library/Application Support/Mendeley Desktop/Downloaded/Kakeda et al. - 2011 - Influence of work shift on glutamic acid and gamma-aminobutyric acid (GABA) Evaluation with proton magnetic reson.pdf:pdf},
issn = {09254927},
journal = {Psychiatry Research - Neuroimaging},
keywords = {3T MRI,Gamma-aminobutyric acid,Glutamate,Glutamine,Proton magnetic resonance spectroscopy,Shift work,brain,shiftWork,sleep,wellbeing,workLifeBalance},
mendeley-tags = {brain,shiftWork,sleep,wellbeing,workLifeBalance},
month = {apr},
number = {1},
pages = {55--59},
pmid = {21377845},
publisher = {Elsevier},
title = {{Influence of work shift on glutamic acid and gamma-aminobutyric acid (GABA): Evaluation with proton magnetic resonance spectroscopy at 3T}},
volume = {192},
year = {2011}
}
@article{Basner2018,
annote = {Analysis of a cross-sectional annual survey conducted by the U.S. Bureau of Labour Statistics (i.e. American Time Use Survey - ATUS). Follow up on the 2014 study. [2003 and 2016, i.e. 5 extra years].

Summary of relevent findings:

Previous study found that "relative to all other waking activities, paid work time was the primary waking activity exchanged for sleep". In this study they isolate the groups that show a sicgnificant increase in sleep duration across survey years either on weekdays or weekends, i.e. employed adults, full time students and retirees (interestingly not for those who are unemployed or not in the labour force). The change is predominantly explained by respondents retiring eariler in the evening: participants seem more willing to trade waking activities for sleep. The results also suggests taht increasing online opportunities to work, learn, bank, shop and perform administrative duties freed up time that likely contributed to increased sleep duration. 

