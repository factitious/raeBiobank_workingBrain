@article{VanLeeuwen2009,
abstract = {Background: Sleep restriction, leading to deprivation of sleep, is common in modern 24-h societies and is associated with the development of health problems including cardiovascular diseases. Our objective was to investigate the immunological effects of prolonged sleep restriction and subsequent recovery sleep, by simulating a working week and following recovery weekend in a laboratory environment. Methods and Findings: After 2 baseline nights of 8 hours time in bed (TIB), 13 healthy young men had only 4 hours TIB per night for 5 nights, followed by 2 recovery nights with 8 hours TIB. 6 control subjects had 8 hours TIB per night throughout the experiment. Heart rate, blood pressure, salivary cortisol and serum C-reactive protein (CRP) were measured after the baseline (BL), sleep restriction (SR) and recovery (REC) period. Peripheral blood mononuclear cells (PBMC) were collected at these time points, counted and stimulated with PHA. Cell proliferation was analyzed by thymidine incorporation and cytokine production by ELISA and RT-PCR. CRP was increased after SR (145{\%} of BL; p {\textless} 0.05), and continued to increase after REC (231{\%} of BL; p{\textless}0.05). Heart rate was increased after REC (108{\%} of BL; p{\textless}0.05). The amount of circulating NK-cells decreased (65{\%} of BL; p{\textless}0.005) and the amount of B-cells increased (121{\%} of BL; p{\textless}0.005) after SR, but these cell numbers recovered almost completely during REC. Proliferation of stimulated PBMC increased after SR (233{\%} of BL; p{\textless}0.05), accompanied by increased production of IL-1$\beta$ (137{\%} of BL; p{\textless}0.05), IL-6 (163{\%} of BL; p{\textless}0.05) and IL-17 (138{\%} of BL; p{\textless}0.05) at mRNA level. After REC, IL-17 was still increased at the protein level (119{\%} of BL; p{\textless}0.05). Conclusions: 5 nights of sleep restriction increased lymphocyte activation and the production of proinflammatory cytokines including IL-1$\beta$ IL-6 and IL-17; they remained elevated after 2 nights of recovery sleep, accompanied by increased heart rate and serum CRP, 2 important risk factors for cardiovascular diseases. Therefore, long-term sleep restriction may lead to persistent changes in the immune system and the increased production of IL-17 together with CRP may increase the risk of developing cardiovascular diseases. {\textcopyright} 2009 van Leeuwen et al.},
author = {van Leeuwen, Wessel M. A. and Lehto, Maili and Karisola, Piia and Lindholm, Harri and Luukkonen, Ritva and Sallinen, Mikael and H{\"{a}}rm{\"{a}}, Mikko and Porkka-Heiskanen, Tarja and Alenius, Harri},
doi = {10.1371/journal.pone.0004589},
editor = {Rogers, Naomi},
file = {:Users/munchausend/Library/Application Support/Mendeley Desktop/Downloaded/van Leeuwen et al. - 2009 - Sleep Restriction Increases the Risk of Developing Cardiovascular Diseases by Augmenting Proinflammatory Res.pdf:pdf},
issn = {1932-6203},
journal = {PLoS ONE},
