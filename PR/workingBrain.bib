doi = {10.1016/j.neuroimage.2013.05.033},
file = {:Users/munchausend/Library/Application Support/Mendeley Desktop/Downloaded/Barch et al. - 2013 - Function in the human connectome Task-fMRI and individual differences in behavior(4).pdf:pdf},
