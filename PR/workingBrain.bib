author = {Byrne, Jamie E.M. and Tremain, Hailey and Leitan, Nuwan D. and Keating, Charlotte and Johnson, Sheri L. and Murray, Greg},
doi = {10.1016/j.neubiorev.2019.01.025},
file = {:Users/munchausend/GoogleDrive/Mendeley/Rae/byrne2019.pdf:pdf},
issn = {18737528},
journal = {Neuroscience and Biobehavioral Reviews},
keywords = {Circadian,Default mode network,Diurnal,Medial prefrontal cortex,Putamen,Reward,Time of day,Ventral striatum,brain,circadianRythm,shiftwork,sleep,systematicReview},
mendeley-tags = {brain,circadianRythm,shiftwork,sleep,systematicReview},
number = {May 2018},
pages = {251--274},
publisher = {Elsevier},
title = {{Circadian modulation of human reward function: Is there an evidentiary signal in existing neuroimaging studies?}},
